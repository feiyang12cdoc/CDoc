<view class="page-body">
  <view class="page-section page-section-gap">
    <map
      id="myMap"
      style="width: 100%; height: 300px;"
      latitude="{{latitude}}"
      longitude="{{longitude}}"
      markers="{{markers}}"
      covers="{{covers}}"
      bindupdated="moveToCurrentLocation"
      bindmarkertap="markertap"
      show-location="true"
      polyline="{{polyline}}"
    ></map>
  </view>
  <view class = "wrap-list" >
    <view class = "wrap-item" wx:for = "{{hospitalList}}" wx:key = "{{index}}"> 
      <!-- <image class="wrap-item-image" src="{{item.doctorImg}}"></image> -->
      <view class="wrap-item-desc" bindtap="getDoctor11">
        <view class="wrap-item-name">{{item.name}}</view>
        <text class="wrap-item-timelist">
          地址：{{item.address}}
          路程时间：{{item.routeWaitingTime}}
          挂号排队时间： {{item.departmentalWaitingTime}}
        </text>
      </view>
    </view>
  </view>
</view>
